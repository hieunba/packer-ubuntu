{
  "variables": {
     "name": "hieunb/lubuntu-1704",
     "build_name": "lubuntu-1704",
     "home": "{{ env `HOME`}}",
     "ssh_username": "vagrant",
     "ssh_password": "vagrant"
  },
  "builders": [{
  "name": "{{ user `build_name` }}",
  "vm_name": "{{ user `build_name` }}",
  "type": "virtualbox-iso",
 "boot_command": [
        "<esc><esc><enter><wait>",
        "/casper/vmlinuz.efi url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/preseed.cfg auto <wait>",
        "priority=critical <wait>",
        "hostname={{ .Name }} <wait>",
        "debian-installer/locale=en_US <wait>",
        "boot=casper automatic-ubiquity <wait>",
        "quiet noprompt noshell <wait>",
        "initrd=/casper/initrd.lz -- <enter><wait>"
      ],
  "guest_os_type": "Ubuntu_64",
  "iso_url": "{{ user `home` }}/Desktop/lubuntu-17.10-desktop-amd64.iso",
  "iso_checksum": "48703ddd81aab8dce53264cb8a8c5202",
  "iso_checksum_type": "md5",
  "http_directory": "http",
  "ssh_username": "{{ user `ssh_username` }}",
  "ssh_password": "{{ user `ssh_password` }}",
  "shutdown_command": "echo '{{ user `ssh_username` }} ' | sudo -S shutdown -P now"
   }]
}
