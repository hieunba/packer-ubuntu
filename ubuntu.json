{
  "variables": {
     "name": "hieu/ubuntu-1704",
     "build_name": "ubuntu-1704",
     "headless": "true",
     "iso_url": "http://cdimage.ubuntu.com/ubuntu/releases/17.10/release/ubuntu-17.10-live-server-amd64.iso",
     "iso_checksum": "67c22be255f5622b0c8d989c2f1e2690",
     "iso_checksum_type": "md5",
     "ssh_username": "vagrant",
     "ssh_password": "vagrant",
     "ssh_timeout": "20m",
     "guest_os_type": "Ubuntu_64"
  },
  "builders": [{
  "type": "virtualbox-iso",
  "boot_command": [
        "<esc><esc><enter><wait>",
        "/install/vmlinuz noapic url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/preseed.cfg <wait>",
        "debian-installer=en_US auto locale=en_US kbd-chooser/method=us <wait>",
        "hostname={{ user `build_name` }} <wait>",
        "fb=false debconf/frontend=noninteractive <wait>",
        "keyboard-configuration/modelcode=SKIP keyboard-configuration/layout=USA keyboard-configuration/variant=USA console-setup/ask_detect=false <wait>",
        "initrd=/install/initrd.gz -- <enter><wait>"
      ],
  "guest_os_type": "{{ user `guest_os_type` }}",
  "guest_additions_path": "/tmp/VBoxGuestAdditions.iso",
  "headless": "{{ user `headless` }}",
  "iso_url": "{{ user `iso_url` }}",
  "iso_checksum": "{{ user `iso_checksum` }}",
  "iso_checksum_type": "{{ user `iso_checksum_type` }}",
  "http_directory": "http",
  "ssh_username": "{{ user `ssh_username` }}",
  "ssh_password": "{{ user `ssh_password` }}",
  "ssh_timeout": "{{ user `ssh_timeout` }}",
  "shutdown_command": "echo 'vagrant' | sudo -S shutdown -P now"
   }],
   "post-processors": [
     {
       "type": "vagrant",
       "keep_input_artifact": true,
       "output": "{{.Provider}}-{{ user `build_name` }}.box"
     }
   ]
}
